<method id="tecs_image_publish.xml">
	<interface>tecs.image.publish</interface>
	<shortdesc lang_cn="镜像发布" lang_en="publish image"/>
	<description lang_cn="镜像发布。" lang_en="publish image."/>
	<parameters>
		<parameter name="session" type="string" lang_cn="登录时由会话管理模块分配的随机字符串。" lang_en="Random string assigned by session manager at login."/>
		<parameter name="id" type="int64" lang_cn="镜像id,可以调用镜像查询接口tecs.image.query查询出所需要的id。" lang_en="image id,can use tecs.image.query show image id."/>
		<parameter name="flag" type="bool" lang_cn="是否公用:" lang_en="public flag:">
			<enum name="0" lang_cn="否" lang_en="NO"/>
			<enum name="1" lang_cn="是" lang_en="YES"/>
		</parameter>
	</parameters>
	<return>
		<success>
			<parameter name="error_code" type="int" lang_cn="错误码，0表示成功。" lang_en="Error code, 0 means success."/>
		</success>
		<fail>
			<parameter name="error_code" type="int" lang_cn="错误码：" lang_en="Error code:">
                <enum name="4002" lang_cn="失败, 镜像正在被使用" lang_en="Failed, image is using"/>
			</parameter>
			<parameter name="error_info" type="string" lang_cn="附加的错误信息。" lang_en="Appending error information."/>
		</fail>
	</return>
	<privilege>
		<user name="Cloud Administrators" lang_cn="TECS云管理员。" lang_en="Cloud administrators of TECS."/>
	</privilege>
	<reference>
		<xref name="tecs.image.enable" href="tecs_image_enable.xml"/>
		<xref name="tecs.image.set" href="tecs_image_set.xml"/>
		<xref name="tecs.image.query" href="tecs_image_query.xml"/>
	</reference>
</method>
