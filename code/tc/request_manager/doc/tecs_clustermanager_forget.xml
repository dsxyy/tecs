<method id="tecs_clustermanager_forget.xml">
	<interface>tecs.cluster.forget</interface>
	<shortdesc lang_cn="遗忘集群" lang_en="cluster forget"/>
	<description lang_cn="遗忘集群,当集群中资源都处于空闲的时候，该接口允许把该集群遗忘。" lang_en="cluster forget，can forget cluster when resource is free."/>
	<parameters>
		<parameter name="session" type="string" lang_cn="登录时由会话管理模块分配的随机字符串。" lang_en="Random string assigned by session manager at login."/>
		<parameter name="name" type="string" lang_cn="集群名称,长度范围 1-64。" lang_en="Cluster name,length range 1-64."/>
	</parameters>
	<return>
		<success>
			<parameter name="error_code" type="int" lang_cn="错误码，0表示成功。" lang_en="Error code, 0 means success."/>
		</success>
		<fail>
			<parameter name="error_code" type="int" lang_cn="错误码：" lang_en="Error code:">
				<enum name="3003" lang_cn="失败, 集群不存在" lang_en="Failed，Cluster does not exist"/>
				<enum name="3005" lang_cn="失败, 集群资源未释放完毕" lang_en="Failed,Cluster resource is using"/>
			</parameter>
			<parameter name="error_info" type="string" lang_cn="附加的错误信息。" lang_en="Appending error information."/>
		</fail>
	</return>
	<privilege>
		<user name="Cloud Administrators" lang_cn="TECS云管理员。" lang_en="Cloud administrators of TECS."/>
	</privilege>
	<reference>
		<xref name="tecs.cluster.disable" href="tecs_clustermanager_disable.xml"/>
		<xref name="tecs.cluster.enable" href="tecs_clustermanager_enable.xml"/>
		<xref name="tecs.cluster.query" href="tecs_clustermanager_query.xml"/>
        <xref name="tecs.cluster.query_core_tcu" href="tecs_clustermanager_query_core_tcu.xml"/>
        <xref name="tecs.cluster.query_netplane" href="tecs_clustermanager_query_netplane.xml"/>
	</reference>
</method>
